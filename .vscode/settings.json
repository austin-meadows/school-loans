{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.nodePath": ".yarn/sdks",
  "files.exclude": {
    "**/.pnp.js": true,
    "**/.yarnrc": true,
    "**/.yarnrc.yml": true
  },
  "prettier.prettierPath": ".yarn/sdks/prettier/index.js",
  "search.exclude": {
    "**/.pnp.*": true,
    "**/.yarn": true
  }
}
