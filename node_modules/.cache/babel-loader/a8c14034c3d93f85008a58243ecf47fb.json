CA,IAAA,IADA,kBACA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,IAAA;AACA;AAHA,GAPA;AAYA,EAAA,IAZA,kBAYA;AACA,WAAA;AACA,MAAA,KAAA,EAAA,KADA;AAEA,MAAA,aAAA,EAAA,KAFA;AAGA,MAAA,aAAA,EAAA,KAHA;AAIA,MAAA,IAAA,EAAA,EAJA;AAKA,MAAA,OAAA,EAAA;AALA,KAAA;AAOA,GApBA;AAqBA,EAAA,OArBA,qBAqBA;AAAA;;AACA,SAAA,aAAA,GAAA,IAAA;AACA,IAAA,EAAA,CAAA,UAAA,CAAA,OAAA,EACA,GADA,CACA,KAAA,IADA,EAEA,GAFA,GAGA,IAHA,CAGA,UAAA,GAAA,EAAA;AACA;AACA;AACA;AACA,MAAA,KAAA,CAAA,aAAA,GAAA,KAAA;AACA,aAAA,GAAA,CAAA,MAAA;AACA,KATA;AAUA,GAjCA;AAkCA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,kBACA,CADA,EACA;AAAA;;AACA,MAAA,CAAA,CAAA,cAAA;AACA,WAAA,aAAA,GAAA,IAAA;AAEA,aAAA,EAAA,CACA,UADA,CACA,OADA,EAEA,GAFA,CAEA,KAAA,IAFA,EAGA,GAHA,CAIA;AACA,QAAA,IAAA,EAAA,KAAA,IADA;AAEA,QAAA,IAAA,EAAA,CAAA,KAAA,KAAA,GAAA,CAAA,GAAA,CAAA,KAAA,KAAA,OAAA,GAAA,CAAA,GAAA,CAAA;AAFA,OAJA,EAQA;AAAA,QAAA,KAAA,EAAA;AAAA,OARA,EAUA,IAVA,CAUA,YAAA;AACA,QAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA,OAZA,EAaA,KAbA,CAaA,YAAA;AACA,QAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA,OAfA,CAAA;AAgBA;AArBA;AAlCA,CAAA","sourcesContent":["<script>\nimport Loading from \"vue-loading-overlay\";\nimport \"vue-loading-overlay/dist/vue-loading.css\";\n\nimport { db } from \"../includes/firebase\";\nimport { Heading, Input, Section } from \"../components\";\n\nexport default {\n  components: {\n    Heading,\n    Input,\n    Loading,\n    Section,\n  },\n  computed: {\n    auth() {\n      return this.$store.state.auth;\n    },\n  },\n  data() {\n    return {\n      donor: false,\n      isLoadingPage: false,\n      isLoadingForm: false,\n      name: \"\",\n      student: false,\n    };\n  },\n  created() {\n    this.isLoadingPage = true;\n    db.collection(\"users\")\n      .doc(this.auth)\n      .get()\n      .then((doc) => {\n        // if (doc.exists) {\n        // } else {\n        // }\n        this.isLoadingPage = false;\n        return doc.exists;\n      });\n  },\n  methods: {\n    submit(e) {\n      e.preventDefault();\n      this.isLoadingForm = true;\n\n      return db\n        .collection(\"users\")\n        .doc(this.auth)\n        .set(\n          {\n            name: this.name,\n            type: (this.donor ? 2 : 0) + (this.student ? 1 : 0),\n          },\n          { merge: true }\n        )\n        .then(() => {\n          this.isLoadingForm = false;\n        })\n        .catch(() => {\n          this.isLoadingForm = false;\n        });\n    },\n  },\n};\n</script>\n\n<template>\n  <Section>\n    <loading loader=\"dots\" :active.sync=\"isLoadingPage\"></loading>\n    <p>Welcome to our platform! Let's get you started.</p>\n    <form ref=\"form\" @submit=\"submit\" novalidate=\"true\">\n      <Heading>I am a</Heading>\n      <Input\n        icon=\"user-graduate\"\n        name=\"student\"\n        placeholder=\"Student\"\n        type=\"checkbox\"\n        v-model=\"student\"\n      />\n      <Input icon=\"donate\" name=\"donor\" placeholder=\"Donor\" type=\"checkbox\" v-model=\"donor\" />\n      <Heading>My name is</Heading>\n      <Input icon=\"signature\" name=\"name\" placeholder=\"Name\" type=\"text\" v-model=\"name\" />\n      <Input icon=\"arrow-right\" :isLoading=\"isLoadingForm\" placeholder=\"Continue\" type=\"submit\" />\n    </form>\n  </Section>\n</template>\n"],"sourceRoot":"src/views"}]}