{
  "name": "school-loans",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^13.2.0",
    "yarn": "^1.19.1"
  },
  "scripts": {
    "__util-build:minify-precache": "terser ./dist/precache-manifest.*.js -m -o ./dist/precache-manifest.*.js",
    "__util-build:minify-sw": "terser ./dist/service-worker.js -m -o ./dist/service-worker.js",
    "__util-build:prod": "cross-env NODE_ENV=production vue-cli-service build",
    "__util-deploy:firebase": "firebase deploy",
    "__util-run:prodserver": "cross-env NODE_ENV=production serve -s ./dist",
    "deploy:prod": "cross-env NODE_ENV=production run-s __util-build:prod __util-build:* __util-deploy:firebase",
    "lint": "vue-cli-service lint",
    "run:dev": "vue-cli-service serve",
    "run:prod": "cross-env NODE_ENV=production run-s __util-build:prod __util-build:* __util-run:prodserver",
    "test:unit": "vue-cli-service test:unit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.26",
    "@fortawesome/free-solid-svg-icons": "5.12.0",
    "@fortawesome/vue-fontawesome": "0.1.9",
    "core-js": "3.6.2",
    "firebase": "7.6.1",
    "register-service-worker": "1.6.2",
    "secure-ls": "1.2.6",
    "vue": "2.6.11",
    "vue-loading-overlay": "3.2.0",
    "vue-router": "3.1.3",
    "vue-toasted": "1.1.27",
    "vuex": "3.1.2",
    "vuex-persistedstate": "2.7.0",
    "vuexfire": "3.2.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.1.2",
    "@vue/cli-plugin-eslint": "4.1.2",
    "@vue/cli-plugin-pwa": "4.1.2",
    "@vue/cli-plugin-router": "4.1.2",
    "@vue/cli-plugin-unit-mocha": "4.1.2",
    "@vue/cli-plugin-vuex": "4.1.2",
    "@vue/cli-service": "4.1.2",
    "@vue/eslint-config-airbnb": "5.0.0",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/test-utils": "1.0.0-beta.30",
    "babel-eslint": "10.0.3",
    "chai": "4.2.0",
    "cross-env": "6.0.3",
    "eslint": "6.8.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-vue": "6.1.2",
    "firebase-tools": "7.11.0",
    "husky": "3.1.0",
    "npm-run-all": "4.1.5",
    "postcss-csso": "4.0.0",
    "postcss-normalize": "8.0.1",
    "prettier": "1.19.1",
    "sass": "1.24.2",
    "sass-loader": "8.0.0",
    "serve": "11.3.0",
    "terser-webpack-plugin": "2.3.1",
    "vue-template-compiler": "2.6.11"
  }
}
