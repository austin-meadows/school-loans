{
  "name": "school-loans",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^13.2.0",
    "yarn": "^1.19.1"
  },
  "scripts": {
    "__util-build:minify-precache": "cross-env NODE_ENV=production terser ./dist/precache-manifest.*.js -m -o ./dist/precache-manifest.*.js",
    "__util-build:minify-sw": "cross-env NODE_ENV=production terser ./dist/service-worker.js -m -o ./dist/service-worker.js",
    "__util-build:prod": "cross-env NODE_ENV=production vue-cli-service build",
    "__util-deploy:firebase": "cross-env NODE_ENV=production firebase deploy",
    "__util-run:prodserver": "cross-env NODE_ENV=production serve -s ./dist",
    "deploy:prod": "cross-env NODE_ENV=production run-s __util-build:prod __util-build:* __util-deploy:firebase",
    "lint": "vue-cli-service lint",
    "run:dev": "vue-cli-service serve",
    "run:prod": "cross-env NODE_ENV=production run-s __util-build:prod __util-build:* __util-run:prodserver",
    "test:unit": "vue-cli-service test:unit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.28",
    "@fortawesome/free-solid-svg-icons": "5.13.0",
    "@fortawesome/vue-fontawesome": "0.1.9",
    "core-js": "3.6.5",
    "firebase": "7.14.6",
    "register-service-worker": "1.7.1",
    "secure-ls": "1.2.6",
    "vue": "2.6.11",
    "vue-loading-overlay": "3.3.2",
    "vue-router": "3.3.2",
    "vue-toasted": "1.1.28",
    "vuex": "3.4.0",
    "vuex-persistedstate": "3.0.1",
    "vuexfire": "3.2.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.4.1",
    "@vue/cli-plugin-eslint": "4.4.1",
    "@vue/cli-plugin-pwa": "4.4.1",
    "@vue/cli-plugin-router": "4.4.1",
    "@vue/cli-plugin-unit-mocha": "4.4.1",
    "@vue/cli-plugin-vuex": "4.4.1",
    "@vue/cli-service": "4.4.1",
    "@vue/eslint-config-airbnb": "5.0.2",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/test-utils": "1.0.3",
    "babel-eslint": "10.1.0",
    "chai": "4.2.0",
    "cross-env": "7.0.2",
    "eslint": "7.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-vue": "6.2.2",
    "firebase-tools": "8.4.1",
    "husky": "4.2.5",
    "npm-run-all": "4.1.5",
    "postcss-csso": "4.0.0",
    "postcss-normalize": "9.0.0",
    "prettier": "2.0.5",
    "sass": "1.26.7",
    "sass-loader": "8.0.2",
    "serve": "11.3.1",
    "terser-webpack-plugin": "3.0.2",
    "vue-template-compiler": "2.6.11"
  }
}
