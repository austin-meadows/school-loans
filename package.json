{
  "name": "school-loans",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "__util-build:minify-precache": "terser ./dist/precache-manifest.*.js -m -o ./dist/precache-manifest.*.js",
    "__util-build:minify-sw": "terser ./dist/service-worker.js -m -o ./dist/service-worker.js",
    "__util-build:prod": "NODE_ENV=production vue-cli-service build",
    "__util-deploy:firebase": "firebase deploy",
    "__util-run:prodserver": "NODE_ENV=production serve -s ./dist",
    "deploy:prod": "run-s __util-build:prod __util-build:* __util-deploy:firebase",
    "lint": "vue-cli-service lint",
    "run:dev": "vue-cli-service serve",
    "run:prod": "run-s __util-build:prod __util-build:* __util-run:prodserver",
    "test:unit": "vue-cli-service test:unit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/vue-fontawesome": "^0.1.7",
    "core-js": "^3.3.3",
    "firebase": "^7.2.1",
    "register-service-worker": "^1.6.2",
    "secure-ls": "^1.2.5",
    "vue": "^2.6.10",
    "vue-loading-overlay": "^3.2.0",
    "vue-router": "^3.0.3",
    "vue-toasted": "^1.1.27",
    "vuex": "^3.1.1",
    "vuex-persistedstate": "^2.5.4",
    "vuexfire": "^3.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.5",
    "@vue/cli-plugin-eslint": "^4.0.5",
    "@vue/cli-plugin-pwa": "^4.0.5",
    "@vue/cli-plugin-router": "^4.0.5",
    "@vue/cli-plugin-unit-mocha": "^4.0.5",
    "@vue/cli-plugin-vuex": "^4.0.5",
    "@vue/cli-service": "^4.0.5",
    "@vue/eslint-config-airbnb": "^4.0.1",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "eslint": "^6.5.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "firebase-tools": "^7.6.1",
    "husky": "^3.0.9",
    "npm-run-all": "^4.1.5",
    "postcss-csso": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "prettier": "^1.18.2",
    "sass": "^1.23.1",
    "sass-loader": "^8.0.0",
    "serve": "^11.2.0",
    "terser-webpack-plugin": "^2.2.1",
    "vue-template-compiler": "^2.6.10"
  }
